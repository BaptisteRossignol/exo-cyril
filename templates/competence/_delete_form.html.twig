<form method="post" action="{{ path('competence_delete', {'id': competence.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette competence?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ competence.id) }}">
    <button>Supprimer</button>
</form>
